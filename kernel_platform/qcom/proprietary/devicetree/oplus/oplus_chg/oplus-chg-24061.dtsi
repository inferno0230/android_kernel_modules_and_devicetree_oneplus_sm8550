	usb_temp_adc_r{
		reg = <PMXR2230_ADC5_GEN3_AMUX2_GPIO4_100K_PU>;
                label = "usb_supplementary_temp_adc ";
                qcom,pre-scaling = <1 1>;
                qcom,ratiometric;
                qcom,hw-settle-time = <200>;
                qcom,scale-fn-type = <OPLUS_ADC_SCALE_HW_CALIB_DEFAULT>;
	};

	/* TODO: the mos themperature is used by which? */
	mos1_con_therm_adc {
		reg = <PMK8550_ADC5_GEN3_AMUX_THM3_GPIO2_100K_PU>;
                label = "mos1_con_therm_adc";
                qcom,pre-scaling = <1 1>;
                qcom,ratiometric;
                qcom,hw-settle-time = <200>;
                qcom,scale-fn-type = <OPLUS_ADC_SCALE_HW_CALIB_DEFAULT>;

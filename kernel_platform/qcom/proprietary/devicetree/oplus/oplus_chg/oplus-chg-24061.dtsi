&qupv3_se4_i2c_pins {
	qupv3_se4_i2c_pulldown: qupv3_se4_i2c_pulldown {
		mux {
			pins = "gpio16", "gpio17";
			function = "gpio";
		};

		config {
			pins = "gpio16", "gpio17";
			drive-strength = <2>;
			bias-pull-down;
			output-low;
		};
	};

	qupv3_se4_i2c_pullup: qupv3_se4_i2c_pullup {
		mux {
			pins = "gpio16", "gpio17";
			function = "gpio";
		};

		config {
			pins = "gpio16", "gpio17";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};
};

&qupv3_se13_i2c_pins {
	qupv3_se13_i2c_sda_active: qupv3_se13_i2c_sda_active {
		mux {
			pins = "gpio52";
			function = "qup1_se5_l0";
		};

		config {
			pins = "gpio52";
			drive-strength = <2>;
			bias-pull-up;
			qcom,i2c_pull; /* 2.2k Pull up */
		};
	};

	qupv3_se13_i2c_scl_active: qupv3_se13_i2c_scl_active {
		mux {
			pins = "gpio53";
			function = "qup1_se5_l1";
		};

		config {
			pins = "gpio53";
			drive-strength = <2>;
			bias-pull-up;
			qcom,i2c_pull; /* 2.2k Pull up */
		};
	};

	qupv3_se13_i2c_sleep: qupv3_se13_i2c_sleep {
		mux {
			pins = "gpio52", "gpio53";
			function = "gpio";
		};

		config {
			pins = "gpio52", "gpio53";
			drive-strength = <2>;
			bias-pull-up;
			qcom,i2c_pull; /* 2.2k Pull up */
		};
	};
};

&pmxr2230_gpios {
	usbtemp_l_gpio_default: usbtemp_l_gpio_default {
		pins = "gpio3"; //GPIO 3
		function = "normal"; //normal
		bias-high-impedance; //DISABLE GPIO3 for ADC
		bias-disable;
	};

	usbtemp_r_gpio_default: usbtemp_r_gpio_default {
		pins = "gpio4"; //GPIO 4
		function = "normal"; //normal
		bias-high-impedance; //DISABLE GPIO4 for ADC
		bias-disable;
	};
};

&pmk8550_gpios {
